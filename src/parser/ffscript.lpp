<STRING>{
	<<EOF>>		{
		char temp[512];
		sprintf(temp, "ERROR: String does not end. LINE %d", yylineno);
		box_eol();box_out(temp); box_eol();
		box_eol();
		BEGIN( INITIAL ); //Don't stay in STRING on next compile!
		return 0;
	}
	[^\"\n\\]*		qstr << yytext;
	[^\"\n\\]*\n	qstr << yytext;
	[^\"\n\\]*\\	{
		yytext[yyleng-1] = 0; //Remove '\'
		qstr << yytext;
		
		register int c = yyinput();
		switch(c)
		{
			case 'a': qstr << '\a'; break;
			case 'b': qstr << '\b'; break;
			case 'f': qstr << '\f'; break;
			case 'n': qstr << '\n'; break;
			case 'r': qstr << '\r'; break;
			case 't': qstr << '\t'; break;
			case 'v': qstr << '\v'; break;
			case '\n': ++yylineno; break;
			case 'x':
			{
				std::stringstream escstr;
				escstr.str("");
				int ch = yyinput();
				if((ch >= '0' && ch <= '9')||(ch>='a'&&ch<='f')||(ch>='A'&&ch<='F'))
					escstr << ((char)ch);
				else
				{
					char temp[512];
					sprintf(temp, "ERROR: Hex-insert escape code '\\x' has no value. LINE %d", yylineno);
					box_eol();box_out(temp); box_eol();
					box_eol();
					BEGIN( INITIAL ); //Don't stay in STRING on next compile!
					return 0;
				}
				ch = yyinput();
				if((ch >= '0' && ch <= '9')||(ch>='a'&&ch<='f')||(ch>='A'&&ch<='F'))
					escstr << ((char)ch);
				else
				{
					unput(ch);
				}
				std::stringstream buf;
				int tmp;
				buf << std::hex << escstr.str();
				buf >> tmp;
				qstr << (char)tmp;
				break;
			}
			case EOF:
			{
				char temp[512];
				sprintf(temp, "ERROR: String does not end. LINE %d", yylineno);
				box_eol();box_out(temp); box_eol();
				box_eol();
				BEGIN( INITIAL ); //Don't stay in STRING on next compile!
				return 0;
			}
			case '\'':
			case '\\':
			case '\"':
			case '\?':
				qstr << (char)c;
				break;
			default:
			{
				char temp[512];
				sprintf(temp, "ERROR: Unknown escape character. LINE %d", yylineno);
				box_eol();box_out(temp); box_eol();
				box_eol();
				BEGIN( INITIAL ); //Don't stay in STRING on next compile!
				return 0;
			}
		}
	}
	[^\"\n\\]*\"	{
		doLines();
		qstr << yytext;
		BEGIN( INITIAL );
		yylval = new ASTString(qstr.str().c_str(), yylloc);
		return QUOTEDSTRING;
	}
}

